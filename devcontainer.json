{
	"name": "pd-imagine",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"forwardPorts": [8888],
	"portsAttributes": {
		"8888": {
			"label": "Jupyter",
			"onAutoForward": "notify"
		}
	},
	"mounts": [
		{"source": "${localEnv:HOME}/.gnupg", "target": "/home/ubuntu/.gnupg", "type": "bind"},
		{"source": "/run/user/1000/gnupg", "target": "/run/user/1000/gnupg", "type": "bind"}
	],
	"customizations": {
		"vscode": {
			"settings": {},
			"extensions": [
				"ms-python.python",
				"ms-toolsai.jupyter"
			]
		}
	}
}
