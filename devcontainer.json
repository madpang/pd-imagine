{
	"name": "pd-imagine",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"forwardPorts": [8888],
	"portsAttributes": {
		"8888": {
			"label": "Jupyter",
			"onAutoForward": "notify"
		}
	},
	"mounts": ["source=/run/user/1000/gnupg/S.gpg-agent,target=/home/panda/.gnupg/S.gpg-agent,type=bind"],	
	"customizations": {
		"vscode": {
			"settings": {},
			"extensions": [
				"ms-python.python",
				"ms-toolsai.jupyter"
			]
		}
	}
}
